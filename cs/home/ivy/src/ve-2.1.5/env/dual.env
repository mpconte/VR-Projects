name cave-1
desc "A simple hmd environment"
option doublebuffer 1
option noborder 1
option stereo 0
wall left {
	loc -0.5 0.0 -1.0
	up 0.0 1.0 0.0
	dir 0.0 0.0 1.0
	size 1.0 1.0
	window left1 {
		slave grendel 
		display default
		geometry 1024x768+0+0
	}
}
wall right {
	loc 0.5 0.0 -1.0
	up 0.0 1.0 0.0
	dir 0.0 0.0 1.0
	size 1.0 1.0
	window right1 {
		slave neotrike
		display default
		geometry 1024x768+0+0
	}
}

